{
  "name": "chi-pinboard-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:marketing": "mkdir -p dist/marketing && cp -r marketing-site/* dist/marketing/",
    "preview": "vite preview",
    "start": "vite preview --host --port 4173",
    "qr": "node scripts/generate-qr.mjs",
    "seed:spots": "node scripts/seed-popular-spots.js",
    "android:sync": "npm run build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build && npx cap sync android && cd android && export JAVA_HOME=/opt/homebrew/opt/openjdk@21 && ./gradlew assembleDebug",
    "android:install": "/opt/homebrew/share/android-commandlinetools/platform-tools/adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:deploy": "npm run android:build && npm run android:install",
    "android:connect": "/opt/homebrew/share/android-commandlinetools/platform-tools/adb connect 192.168.1.202:5555"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/filesystem": "^7.1.4",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.58.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.3",
    "isomorphic-dompurify": "^2.28.0",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^2.4.0",
    "leaflet.heat": "^0.2.0",
    "leaflet.offline": "^3.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "openai": "^5.23.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.30.1",
    "sql.js": "^1.13.0",
    "stripe": "^14.7.0",
    "supercluster": "^8.0.1",
    "uuid": "^13.0.0",
    "vite": "^5.4.9"
  },
  "devDependencies": {
    "vercel": "^48.1.6"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "not op_mini all"
  ]
}
